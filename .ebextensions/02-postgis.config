# GDAL/Postgis stuff
commands:
  01_yum_update:
    command: yum -y update
  02_epel_repo:
    command: yum-config-manager -y --enable epel
  03_remi_repo:
    command: yum-config-manager -y --enable remi
packages:
  yum:
    git: []
    postgresql-libs: []
    postgresql-contrib: []
    postgresql-devel: []